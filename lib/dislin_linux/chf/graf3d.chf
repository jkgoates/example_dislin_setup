void graf3d(double xa, double xe, double xorg, double xstp, double ya, double ye, double yorg, double ystp, double za, double ze, double zorg, double zstp) {
    void *fptr;

    fptr = dlsym(_Chdislin_handle, "graf3d_chdl");
    if(fptr == NULL) {
        fprintf(_stderr, "Error: %s(): dlsym(): %s\n", __func__,  dlerror());
        return;
    }
    dlrunfun(fptr, NULL, graf3d, xa, xe, xorg, xstp, ya, ye, yorg, ystp, za, ze, zorg, zstp);
}
