void shdafr(int *inray, long *ipray, int *icray, int n) {
    void *fptr;

    fptr = dlsym(_Chdislin_handle, "shdafr_chdl");
    if(fptr == NULL) {
        fprintf(_stderr, "Error: %s(): dlsym(): %s\n", __func__,  dlerror());
        return;
    }
    dlrunfun(fptr, NULL, shdafr, inray, ipray, icray, n);
}
