double y2dpos(double x, double y) {
    void *fptr;
    double retval;

    fptr = dlsym(_Chdislin_handle, "y2dpos_chdl");
    if(fptr == NULL) {
        fprintf(_stderr, "Error: %s(): dlsym(): %s\n", __func__,  dlerror());
        return -1;
    }
    dlrunfun(fptr, &retval, y2dpos, x, y);
    return retval;
}
