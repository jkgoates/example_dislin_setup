void tube3d(double x1, double y1, double z1, double x2, double y2, double z2, double r, int nsk1, int nsk2) {
    void *fptr;

    fptr = dlsym(_Chdislin_handle, "tube3d_chdl");
    if(fptr == NULL) {
        fprintf(_stderr, "Error: %s(): dlsym(): %s\n", __func__,  dlerror());
        return;
    }
    dlrunfun(fptr, NULL, tube3d, x1, y1, z1, x2, y2, z2, r, nsk1, nsk2);
}
